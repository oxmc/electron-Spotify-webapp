{
  "name": "electron-spotify-webapp",
  "version": "1.0.3",
  "description": "An auto-updating Spotify app for raspberry pi, built with electron-builder.",
  "main": "src/js/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/oxmc/electron-Spotify-webapp.git"
  },
  "scripts": {
